<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo_chichen_2">


    <EditText
        android:id="@+id/decimal_numbers_text"
        style="@style/Widget.AppCompat.AutoCompleteTextView"
        android:layout_width="260dp"
        android:layout_height="44dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="10dp"
        android:background="@color/design_default_color_background"
        android:ems="10"
        android:text=""
        android:inputType="none"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.429"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.031" />

    <Button
        android:id="@+id/n0_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/n0"
        android:text=""
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/decimal_numbers_text" />

    <Button
        android:id="@+id/n1_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/n1"
        android:text=""
        app:layout_constraintStart_toEndOf="@+id/n0_btn"
        app:layout_constraintTop_toBottomOf="@+id/decimal_numbers_text" />

    <Button
        android:id="@+id/n2_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/n2"
        android:text=""
        app:layout_constraintStart_toEndOf="@+id/n1_btn"
        app:layout_constraintTop_toBottomOf="@+id/decimal_numbers_text" />

    <Button
        android:id="@+id/n3_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/n3"
        android:text=""
        app:layout_constraintStart_toEndOf="@+id/n2_btn"
