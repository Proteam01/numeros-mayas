        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="="
        app:layout_constraintEnd_toEndOf="@+id/n18_btn"
        app:layout_constraintTop_toBottomOf="@+id/n18_btn" />

    <Button
        android:id="@+id/clear_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="C"
        app:layout_constraintStart_toStartOf="@+id/n17_btn"
        app:layout_constraintTop_toBottomOf="@+id/n17_btn" />


</androidx.constraintlayout.widget.ConstraintLayout>